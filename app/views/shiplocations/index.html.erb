<p id="notice"><%= notice %></p>

<h1>Listing Shiplocations</h1>

<table>
  <thead>
    <tr>
      <th>Country</th>
      <th>Price</th>
      <th>Cost</th>
      <th>Listing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shiplocations.each do |shiplocation| %>
      <tr>
        <td><%= shiplocation.country %></td>
        <td><%= shiplocation.price %></td>
        <td><%= shiplocation.cost %></td>
        <td><%= shiplocation.listing %></td>
        <td><%= link_to 'Show', shiplocation %></td>
        <td><%= link_to 'Edit', edit_shiplocation_path(shiplocation) %></td>
        <td><%= link_to 'Destroy', shiplocation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shiplocation', new_shiplocation_path %>
