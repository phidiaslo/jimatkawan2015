<%= simple_form_for(@shiplocation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :country %>
    <%= f.input :price %>
    <%= f.input :cost %>
    <%= f.association :listing %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
